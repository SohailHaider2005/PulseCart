{
    "version": 2,
    "builds": [
        {
          "src": "./backend/server.js",
          "use": "@vercel/node",
          "buildCommand": "yarn build"
        },
        {
          "src": "./frontend/build",
          "use": "@vercel/static"
        }
      ],
   
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/backend/server.js"
        }
    ]
}